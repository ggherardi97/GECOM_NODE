  <script>

      function redirectToProcess() {
          window.location = "ProcessDetail";
      }

      $(document).ready(function () {
          $("#pageName").text("Processos");
          $("#subpageName").text("Processos");

          //filtro por coluna
          $('#process thead input').on('keyup change', function () {
              var index = $(this).parent().index();
              var value = $(this).val().toLowerCase();

              $('#process tbody tr').filter(function () {
                  $(this).toggle(
                      $(this).find('td').eq(index).text().toLowerCase().indexOf(value) > -1
                  );
              });
          });

          const actionIndex = 8; 
          const table = $('#process');
          $('#actionFilter').on('change', function () {
              const selected = $(this).val().toLowerCase();

              table.find('tbody tr').each(function () {
                  const cell = $(this).find('td').eq(actionIndex).text().toLowerCase();
                  if (selected === '' || cell.indexOf(selected) > -1) {
                      $(this).show();
                  } else {
                      $(this).hide();
                  }
              });
          });
      });
  </script>